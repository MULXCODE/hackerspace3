- content_for :title, "Entries to #{@challenge.name}"
%main.challenge_page
  %h2= yield :title
  - if @competition.either_judging_window_open? LAST_TIME_ZONE
    - if @teams.present?
      = render 'team_judging_table'
    - else
      %p No team entries to display for this challenge.
  - elsif @competition.started?(@region.time_zone) || @region.international?
    = render 'team_checkpoint_table'
  - else
    %p Challenge entries will appear here when the competition starts.
