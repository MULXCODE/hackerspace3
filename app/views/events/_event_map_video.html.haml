- unless @event.place_id.blank?
  %iframe{:allowfullscreen => "", :frameborder => "0", :height => "450", :src => "https://www.google.com/maps/embed/v1/place?q=place_id:#{@event.place_id}=#{ENV['GOOGLE_API_KEY']}", :style => "border:0", :width => "600"}
  %br/
- unless @event.video_id.blank?
  %iframe#ytplayer{:frameborder => "0", :height => "360", :src => "https://www.youtube.com/embed/#{@event.video_id}", :type => "text/html", :width => "640"}
  %br/
