<article>
	<div>
	<h3><%= event.name %></h3>
	<h4><%= event.region.name %></h4>
	<% unless event.start_time.nil? || event.end_time.nil? %>
		<h4><%= event.start_time.strftime("%e %B %Y") %></h4>
		<h4><%= event.start_time.strftime("%I.%M %p") %></h4>
	<% end %>
	<% if event.type == STATE_CONNECTION %>
		<p>Connection Event</p>
	<% end %>
	</div>
</article>
