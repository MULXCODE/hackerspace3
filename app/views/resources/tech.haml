- content_for :title, "Tech for #{@competition.name}"
%main.dataset_page
  %h2= yield :title
  %section.dataportals
    - @tech.each do |resource|
      = render 'resource_tile',          |
        url: resource['url'],            |
        name: resource['name'],          |
        short_url: resource['short_url']
= link_to 'Back to Resources', resources_path
