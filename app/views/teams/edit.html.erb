<h2>Update <%= @team.current_project.team_name %> Participating Competition Event</h2>

<%= form_for @team do |form| %>
  <%= form.label :event_id %>
  <%= form.select :event_id, options_from_collection_for_select(@events, :id, :name, @team.event_id.to_s) %><br>
  <div class="field">
    <%= form.label :published %>
    <%= form.check_box :published %>
  </div>
  <%= form.submit %>
<% end %>
<%= link_to 'Back to Team', team_path(@team) %>
