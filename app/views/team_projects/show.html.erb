<h2><%= @current_version.team_name %></h2>

<h3>Team Leader</h3>
<%= render 'users/user_contact', locals: { user: @team_project.team_leader } %>
<h3>Team Members</h3>

<% if (team_members = @team_project.team_members).present? %>
  <% team_members.each do |team_member| %>
    <%= render 'users/user_contact', locals: { user: team_member } %>
  <% end %>
<% end %>

<%= link_to 'Add Team Members', new_team_project_assignment_path(@team_project, title: TEAM_MEMBER) %><br>
<%= link_to 'Back to Event', event_path(@team_project.event.identifier) %><br>
