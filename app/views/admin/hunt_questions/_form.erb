<%= form_for [:admin, @competition, @hunt_question] do |form| %>
  <div class="form-group">
    <%= form.label :question %>
    <%= form.text_field :question %>
  </div>
  <div class="form-group">
    <%= form.label :answer %>
    <%= form.text_field :answer %>
  </div>
  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
