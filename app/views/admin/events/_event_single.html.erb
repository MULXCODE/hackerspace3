<% event = locals[:event] %>
<% region = event.region %>
<h4><%= "#{event.name} in #{region.name}" %></h4>
<% unless event.start_time.blank? %>
  Start Time: <%= event.start_time %><br>
<% end %>
<% unless event.end_time.blank? %>
  End Time: <%= event.end_time %><br>
<% end %>
<%= link_to 'Go to Event', admin_region_event_path(region, event) %><br>
