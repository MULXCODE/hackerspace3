%table#national-challenge-entries-table
  %thead
    %th Challenges
    - @event_names.each do |event_name|
      %th= event_name
    %th Total Entries
  %tbody
    - @challenge_names.each do |challenge_name|
      %tr
        %td= challenge_name
        - @event_names.each do |event_name|
          %td= @event_counts[challenge_name][event_name]
        %td= @event_counts[challenge_name][:total_entries]
