= form_for [:admin, @region, @data_set] do |form|
  #hiddendescription
  .form-group
    = form.label :url
    = form.url_field :url
  .form-group
    = form.label :name
    = form.text_field :name
  .form-group
    = form.label :description
    = form.text_area :description
  .actions
    = form.submit
