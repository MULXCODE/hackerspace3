<%= form_for [:admin, @competition] do |form| %>
  <div class="form-group">
    <%= form.label :year %>
    <%= form.number_field :year %>
  </div>
  <div class="form-group">
    <%= form.label :team_form_start %>
    <%= form.datetime_local_field :team_form_start %>
  </div>
  <div class="form-group">
    <%= form.label :team_form_end %>
    <%= form.datetime_local_field :team_form_end %>
  </div>
  <div class="form-group">
    <%= form.label :start_time %>
    <%= form.datetime_local_field :start_time %>
  </div>
  <div class="form-group">
    <%= form.label :end_time %>
    <%= form.datetime_local_field :end_time %>
  </div>
  <div class="form-group">
    <%= form.label :peoples_choice_start %>
    <%= form.datetime_local_field :peoples_choice_start %>
  </div>
  <div class="form-group">
    <%= form.label :peoples_choice_end %>
    <%= form.datetime_local_field :peoples_choice_end %>
  </div>
  <div class="form-group">
    <%= form.label :challenge_judging_start %>
    <%= form.datetime_local_field :challenge_judging_start %>
  </div>
  <div class="form-group">
    <%= form.label :challenge_judging_end %>
    <%= form.datetime_local_field :challenge_judging_end %>
  </div>
  <div class="custom-control custom-checkbox">
    <%= form.check_box :current, class:'custom-control-input' %>
    <%= form.label :current, class: 'custom-control-label' %>
  </div>
  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
