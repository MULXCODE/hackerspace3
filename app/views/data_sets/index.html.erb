<h3>Dataset Highlight</h3>

<%= link_to 'Download CSV', data_sets_path(format: 'csv') %>

<table id="dataset_table">
	<thead>
		<tr>
			<th>Name</th>
			<th>Description</th>
			<th>Dataset URL</th>
			<th>Jurisdiction</th>
		</tr>
	</thead>
	<tbody>

<% @data_sets.order(:name).each do |data_set| %>
		<tr>
			<td><%= link_to data_set.name, data_set_path(data_set) %></td>
			<td><%= markdown data_set.description %></td>
			<td><%= link_to(data_set.url, data_set.url, {:class => "dataseturl"}) %></td>
			<td><%= @id_regions[data_set.region_id].name %></td>
		</tr>
<% end %>
	</tbody>
</table>
<script type="text/javascript" src="https://cdn.datatables.net/1.10.19/js/jquery.dataTables.min.js"></script>
<link href="https://cdn.datatables.net/1.10.19/css/jquery.dataTables.min.css" rel="stylesheet">
